  "license": "ISC",
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "concurrently": "^8.2.2",
    "focus-trap": "^7.5.4",
    "tailwindcss": "^3.4.10",
    "webpack": "^5.94.0",
    "webpack-cli": "^5.1.4"
  }
}
